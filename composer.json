{
    "name": "sysotel/sys-common-php",
    "repositories": [
        {
            "url": "https://github.com/delta4op/laravel-mongodb",
            "type": "git"
        }
    ],
    "require": {
        "php": "^8.1",
        "alexanderpas/http-enum": "^1.0",
        "delta4op/laravel-mongodb": "dev-main",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/contracts": "^9.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26",
        "ext-mongodb": "1.13.*"
    },
    "autoload": {
        "psr-4": {
            "SYSOTEL\\APP\\Common\\": "src"
        },
        "files":  [
            "src/Helpers/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SYSOTEL\\APP\\Common\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SYSOTEL\\APP\\Common\\ServiceProvider"
            ],
            "aliases": {

            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
